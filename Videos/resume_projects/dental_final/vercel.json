{
  "version": 2,
  "builds": [
    {
      "src": "html/**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "assets/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*\\.html)",
      "dest": "/html/$1"
    },
    {
      "src": "/",
      "dest": "/html/home.html"
    },
    {
      "src": "/patients",
      "dest": "/html/patients.html"
    },
    {
      "src": "/register",
      "dest": "/html/register.html"
    },
    {
      "src": "/materials",
      "dest": "/html/materials.html"
    },
    {
      "src": "/note",
      "dest": "/html/note.html"
    },
    {
      "src": "/invoice",
      "dest": "/html/invoice.html"
    },
    {
      "src": "/card",
      "dest": "/html/card.html"
    },
    {
      "src": "/challan",
      "dest": "/html/challan.html"
    },
    {
      "src": "/exp",
      "dest": "/html/exp.html"
    },
    {
      "src": "/salary",
      "dest": "/html/salary.html"
    }
  ],
  "rewrites": [
    {
      "source": "/assets/:path*",
      "destination": "/assets/:path*"
    },
    {
      "source": "/:path*",
      "destination": "/html/:path*"
    }
  ]
}
